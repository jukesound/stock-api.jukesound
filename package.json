{
  "name": "stock-api.jukesound",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./src/bin/www",
    "server": "nodemon --exec babel-node ./src/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "migrate": "babel-node node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "babel-node node_modules/.bin/sequelize db:migrate:undo",
    "seed": "babel-node node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "babel-node node_modules/.bin/sequelize db:seed:undo:all",
    "db:reset": "npm run migrate:undo && npm run migrate && npm run seed",
    "docker:db:reset": "docker-compose exec jss_node npm run db:reset",
    "docker": "docker-compose exec jss_node sh",
    "test": "eslint --ext .js . && jest",
    "generate-docs": "rm -rf docs/ && node_modules/.bin/jsdoc -c .jsdoc.json -r ."
  },
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "dotenv": "^7.0.0",
    "express": "~4.16.4",
    "faker": "^4.1.0",
    "joi": "^14.3.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "pg": "^7.10.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.6.1",
    "sequelize-cli": "^5.4.0",
    "slugify": "^1.3.4",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.11",
    "@types/joi": "^14.3.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.8",
    "@types/sequelize": "^4.27.48",
    "@types/supertest": "^2.0.7",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.18.11",
    "sequelize-typescript": "^0.6.10",
    "supertest": "^4.0.2",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "tui-jsdoc-template": "^1.2.2",
    "typescript": "^3.4.5"
  }
}
