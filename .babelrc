{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "@root": "./",

        "@database": "./database",

        "@config": "./config",

        "@src": "./src",
        "@routes": "./src/routes",
        "@utils": "./src/utils"
      }
    }]
  ]
}
